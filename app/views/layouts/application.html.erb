<!DOCTYPE html>
<html>
  <head>
    <!-- Latest compiled and minified CSS -->
    
    <title>VoteOnIt</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

<body>
  <header>
  
  <nav class= "topnav">
    <ul>
      <%if current_user%>
        <li><%=link_to "Log Out", logout_path, method: :delete%></li>
    
  
        <li><%=link_to "Create New Subject", new_subject_path%> </li>

        <li><%=link_to "See all subjects", subjects_path%></li>

        <li><%=link_to "Home", "/users/#{current_user.id}"%></li>
      <%end%>
    </ul>
  </nav>
  
  </header>
 
  
  <%= yield %>
  </body>
</html>
