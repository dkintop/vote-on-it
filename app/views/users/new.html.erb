<h1>Sign Up</h1>

<%=user_error_messages%>

<%= form_for @user  do |f| %>
    <%= f.label :email  %>
    <%=f.email_field :email%>
    <br>
    <%=f.label :password%>
    <%=f.password_field :password%>
    <br>
    <%=f.label :name%>
    <%=f.text_field :name%>
    <br>
    <%=f.submit "Sign Up"%> 
<% end %>