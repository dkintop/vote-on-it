
<%if params[:subject_id]%>
    <h3>Thanks For Voting! See how others voted:</h3>
<%end%>



<%@votes.each do |vote|%>
    <div class='index_item'>
    
    <h3><%=vote.subject.title%><h3>
    <p> <%=vote.user.name%> voted: <%=vote.option_choice%></p>
    <div class='vote_link'>
        <%=link_to "View Subject", subject_path(vote.subject)%>
    </div>
    
    </div>

    <%end%>
